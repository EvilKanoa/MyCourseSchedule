{
  "name": "mycourseschedule",
  "version": "0.1.0",
  "description": "An online interface to schedule your courses, originally built for students of UoG, WLU, and UW.",
  "private": true,
  "license": "MIT",
  "scripts": {
    "test": "echo \"I'm lazy and need to add tests...\" && exit 0",
    "dev": "yarn start",
    "start": "NODE_ENV=development webpack-dev-server --env.WEBADVISOR_API=https://webadvisor-api.herokuapp.com/ --config webpack.dev.js --colors --progress --no-info",
    "build": "NODE_ENV=production webpack --env.WEBADVISOR_API=https://webadvisor-api.herokuapp.com/ --config webpack.prod.js",
    "prod": "NODE_ENV=production http-server \"./dist\" -p ${PORT:=80} --utc --push-state",
    "prettier": "prettier --write \"src/**/*.js\"",
    "prettier-check": "prettier --check \"src/**/*.js\"",
    "heroku-postbuild": "echo Skip builds on Heroku"
  },
  "engines": {
    "node": ">=10"
  },
  "author": "Kanoa Haley",
  "browserslist": [
    "> 0.5%"
  ],
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "consistent",
    "trailingComma": "all",
    "arrowParens": "avoid"
  },
  "dependencies": {
    "spa-http-server": "0.9.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/plugin-proposal-class-properties": "7.4.4",
    "@babel/plugin-proposal-decorators": "7.4.4",
    "@babel/polyfill": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "autoprefixer": "9.6.0",
    "babel-loader": "8.0.6",
    "babel-plugin-lodash": "3.3.4",
    "classnames": "2.2.6",
    "clean-webpack-plugin": "0.1.19",
    "core-js": "3.1.4",
    "css-loader": "1.0.1",
    "file-loader": "2.0.0",
    "graphql-hooks": "3.5.1",
    "graphql-hooks-memcache": "1.2.0",
    "html-webpack-plugin": "3.2.0",
    "lodash": "4.17.11",
    "mini-css-extract-plugin": "0.7.0",
    "node-sass": "4.12.0",
    "postcss-loader": "3.0.0",
    "prettier": "1.18.2",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-icons": "3.7.0",
    "react-infinite-scroller": "^1.2.4",
    "react-paginate": "6.3.0",
    "react-redux": "7.1.0",
    "react-router-dom": "4.3.1",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "sass-loader": "7.1.0",
    "sifter": "0.5.3",
    "webpack": "4.34.0",
    "webpack-cli": "3.3.4",
    "webpack-dev-server": "3.7.2",
    "webpack-merge": "4.2.1"
  }
}
