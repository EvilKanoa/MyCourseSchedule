{
  "name": "mycourseschedule",
  "version": "0.0.1",
  "description": "An online interface to schedule your courses, originally built for students of UoG",
  "private": true,
  "license": "MIT",
  "scripts": {
    "test": "echo \"I'm lazy and need to add tests...\" && exit 0",
    "dev": "npm start",
    "start": "NODE_ENV=development webpack-dev-server --open --config webpack.dev.js --colors --progress --no-info",
    "build": "webpack --config webpack.prod.js",
    "prod": "NODE_ENV=production npm run build && http-server \"./dist\" -p ${PORT:=80} --utc --push-state"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=4"
  },
  "author": "Kanoa Haley",
  "browserslist": "> 0.25%, not dead",
  "dependencies": {
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-decorators": "7.1.2",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "babel-loader": "8.0.4",
    "clean-webpack-plugin": "0.1.19",
    "css-loader": "1.0.1",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "3.2.0",
    "mini-css-extract-plugin": "0.4.4",
    "node-sass": "4.9.4",
    "react": "16.6.0",
    "react-dom": "16.6.0",
    "react-redux": "5.1.0",
    "react-router-dom": "4.3.1",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0",
    "sass-loader": "7.1.0",
    "spa-http-server": "0.9.0",
    "webpack": "4.23.1",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10",
    "webpack-merge": "4.1.4"
  }
}
